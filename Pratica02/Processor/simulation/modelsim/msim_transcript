wave modify -driver freeze -pattern constant -value 0000000110101011 -range 15 0 -starttime 2800ps -endtime 4000ps Edit:/Processor/DIN
wave modify -driver freeze -pattern constant -value 0000000101011101 -range 15 0 -starttime 2800ps -endtime 4000ps Edit:/Processor/DIN
run -all
wave modify -driver freeze -pattern constant -value 0000000101101011 -range 15 0 -starttime 2800ps -endtime 4000ps Edit:/Processor/DIN
restart
run -all
wave modify -driver freeze -pattern constant -value 001010000 -range 15 0 -starttime 3200ps -endtime 4000ps Edit:/Processor/DIN
