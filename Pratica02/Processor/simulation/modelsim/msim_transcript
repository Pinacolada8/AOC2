wave create -driver freeze -pattern clock -initialvalue 0 -period 100ps -dutycycle 50 -starttime 0ps -endtime 4000ps sim:/Processor/Clock
wave create -driver freeze -pattern constant -value 001101000 -range 15 0 -starttime 0ps -endtime 4000ps sim:/Processor/DIN
add wave -position insertpoint  \
sim:/Processor/R
wave modify -driver freeze -pattern constant -value 10110110101 -range 15 0 -starttime 100ps -endtime 4000ps Edit:/Processor/DIN
wave modify -driver freeze -pattern constant -value 000011101 -range 15 0 -starttime 400ps -endtime 4000ps Edit:/Processor/DIN
wave modify -driver freeze -pattern constant -value 010101011 -range 15 0 -starttime 800ps -endtime 4000ps Edit:/Processor/DIN
